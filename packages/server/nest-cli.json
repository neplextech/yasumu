{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "generateOptions": {
    "baseDir": "modules"
  },
  "compilerOptions": {
    "deleteOutDir": true,
    "builder": "swc",
    "manualRestart": true,
    "typeCheck": true
  }
}
