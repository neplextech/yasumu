{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    "process:default",
    "core:window:default",
    "core:window:allow-start-dragging",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://*"
        },
        {
          "url": "https://*"
        },
        {
          "url": "http://*:*"
        },
        {
          "url": "https://*:*"
        }
      ]
    }
  ]
}
