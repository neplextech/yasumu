{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Yasumu Canary",
  "version": "0.0.2",
  "identifier": "com.neplextech.yasumu.canary",
  "build": {
    "beforeDevCommand": "pnpm run bundle && pnpm dev",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "pnpm run bundle && pnpm build",
    "frontendDist": "../out"
  },
  "app": {
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "resources/"
    ],
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDc3QjhEMEY5NkI2RDQ3RjEKUldUeFIyMXIrZEM0ZHhlZTIreVl5RTFHeTJJUzRvZU1CemhBL28zZTIzdkNrRzZzRS9TL2dDbXQK",
      "endpoints": [
        "https://github.com/neplextech/yasumu/releases/latest/download/latest.json"
      ]
    }
  }
}